{
  "name": "dr-kunta",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start": "sls offline --stage dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "clear-espoo-signs": "node testFiles/uploadTestFile.js ./testFiles/empty.json dr-kunta-dev-bucket geojson/espoo/trafficSigns/ .json",
    "clear-espoo-obstacles": "node testFiles/uploadTestFile.js ./testFiles/empty.json dr-kunta-dev-bucket geojson/espoo/obstacles/ .json",
    "espoo-obstacles": "sls invoke local --function fetchAndParseData -e OFFLINE=true --data '{ \"url\": \"https://kartat.espoo.fi/trimbleogcapi\", \"format\": \"json\", \"municipality\": \"espoo\", \"assetTypes\": { \"obstacles\": \"infrao:Rakenne\" }}'",
    "espoo-signs": "sls invoke local --function fetchAndParseData -e OFFLINE=true --data '{ \"url\": \"https://kartat.espoo.fi/trimbleogcapi\", \"format\": \"json\", \"municipality\": \"espoo\", \"assetTypes\": { \"trafficSigns\": \"infrao:Liikennemerkki\" }}'",
    "helsinki-signs": "sls invoke local --function fetchAndParseData -e OFFLINE=true --data '{ \"url\": \"https://city-infra.test.hel.ninja/v1\", \"format\": \"helsinki\", \"municipality\": \"helsinki\", \"assetTypes\": { \"trafficSigns\": \"traffic-sign-reals\" }}'",
    "clear-helsinki-signs": "node testFiles/uploadTestFile.js ./testFiles/empty.json dr-kunta-dev-bucket geojson/helsinki/trafficSigns/ .json",
    "helsinki-prod-signs":  "sls invoke local --function fetchAndParseData -e OFFLINE=true --data '{ \"url\": \"https://cityinfra.api.hel.fi/v1\", \"format\": \"helsinki\", \"municipality\": \"helsinki\", \"assetTypes\": { \"trafficSigns\": \"traffic-sign-reals\" }}'"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.300.0",
    "@aws-sdk/client-s3": "^3.300.0",
    "@aws-sdk/client-scheduler": "^3.312.0",
    "@aws-sdk/client-ssm": "^3.300.0",
    "@aws-sdk/lib-storage": "^3.300.0",
    "@aws-sdk/s3-request-presigner": "^3.300.0",
    "@middy/core": "^2.5.3",
    "@middy/http-error-handler": "^4.3.1",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/validator": "^4.3.1",
    "@turf/boolean-point-on-line": "^7.1.0",
    "@turf/helpers": "^7.1.0",
    "@types/proj4": "^2.5.5",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.8",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.7",
    "ol": "^10.1.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "pg-native": "^3.0.1",
    "proj4": "^2.12.1",
    "wkx": "^0.5.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/ejs": "^3.1.5",
    "@types/node": "^18.15.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.6.5",
    "@types/serverless": "^3.12.22",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "esbuild": "^0.14.11",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.7.1",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^9.2.0",
    "serverless-s3-local": "^0.7.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
